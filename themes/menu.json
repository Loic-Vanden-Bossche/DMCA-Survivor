{
  "#10_text":
  {
    "colours":
       {
         "normal_text": "#32a852"
       },
    "font":
    {
      "name": "montserrat",
      "size": "10",
      "bold": "1",
      "italic": "0",
      "regular_resource": { "package": "graphics.fonts", "resource": "Montserrat-Regular.ttf"},
      "bold_resource": { "package": "graphics.fonts", "resource": "Montserrat-Bold.ttf"},
      "italic_resource": { "package": "graphics.fonts", "resource": "Montserrat-Italic.ttf"},
      "bold_italic_resource": { "package": "graphics.fonts", "resource": "Montserrat-BoldItalic.ttf"}
    }
  },
  "#20_text":
  {
    "font":
    {
      "name": "montserrat",
      "size": "20",
      "bold": "1",
      "italic": "0",
      "regular_resource": { "package": "graphics.fonts", "resource": "Montserrat-Regular.ttf"},
      "bold_resource": { "package": "graphics.fonts", "resource": "Montserrat-Bold.ttf"},
      "italic_resource": { "package": "graphics.fonts", "resource": "Montserrat-Italic.ttf"},
      "bold_italic_resource": { "package": "graphics.fonts", "resource": "Montserrat-BoldItalic.ttf"}
    }
  },
  "#30_text":
  {
    "font":
    {
      "name": "montserrat",
      "size": "30",
      "bold": "1",
      "italic": "0",
      "regular_resource": { "package": "graphics.fonts", "resource": "Montserrat-Regular.ttf"},
      "bold_resource": { "package": "graphics.fonts", "resource": "Montserrat-Bold.ttf"},
      "italic_resource": { "package": "graphics.fonts", "resource": "Montserrat-Italic.ttf"},
      "bold_italic_resource": { "package": "graphics.fonts", "resource": "Montserrat-BoldItalic.ttf"}
    }
  },
  "#start_button":
  {
    "misc":
    {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "4",
      "border_width": "1",
      "shadow_width": "2",
      "state_transitions":
      {
          "normal_hovered": "0.2",
          "hovered_normal": "0.5"
      }
    }
  },

  "panel":
  {
    "misc":
    {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "4",
      "border_width": "1",
      "shadow_width": "2"
    }
  },

  "#drop_down_shape_prototype":
  {
    "misc": {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "4"
    }
  },
  "drop_down_menu":
  {
    "prototype": "#drop_down_shape_prototype",

    "misc":
    {
      "shadow_width": "2",
      "border_width": "1",
      "expand_direction": "up",
      "open_button_width": "0"
    }
  },

  "drop_down_menu.#drop_down_options_list":
  {
    "prototype": "#drop_down_shape_prototype",

    "misc":
    {
      "shadow_width": "2",
      "border_width": "1"
    }
  },

  "drop_down_menu.#drop_down_options_list.vertical_scroll_bar":
  {
    "misc":
    {
      "shadow_width": "0",
      "border_width": "1",
      "enable_arrow_buttons": "0"
    }
  },

  "drop_down_menu.#drop_down_options_list.button":
  {
    "misc":
    {
      "shadow_width": "0",
      "border_width": "0",

      "state_transitions":
      {
        "normal_hovered": "0.2",
        "hovered_normal": "0.5"
      }
    }
  },

  "drop_down_menu.button":
  {
    "misc":
    {
      "shadow_width": "0",
      "border_width": "0",
      "state_transitions":
      {
        "normal_hovered": "0.2",
        "hovered_normal": "0.5"
      }
    }
  }
}
